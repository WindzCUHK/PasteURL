{
    "name": "pasteurl",
    "displayName": "Markdown PasteURL",
    "description": "Paste URL in markdown",
    "version": "0.2.2",
    "publisher": "kukushi",
    "repository": {
        "type": "git",
        "url": "https://github.com/kukushi/PasteURL.git"
    },
    "bugs": {
        "url": "https://github.com/kukushi/PasteURL/issues"
    },
    "author": "kukushi",
    "license": "MIT",
    "icon": "images/icon.svg",
    "homepage": "https://github.com/kukushi/PasteURL/blob/master/README.md",
    "engines": {
        "vscode": "^1.8.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pasteURL"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.pasteURL",
            "title": "Paste URL"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "get-title": "^1.0.1",
        "hyperquest": "^2.1.2"
    }
}