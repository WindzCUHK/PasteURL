{
    "name": "pasteurl",
    "displayName": "Markdown PasteURL",
    "description": "Paste URL in markdown",
    "version": "0.3.9",
    "publisher": "kukushi",
    "repository": {
        "type": "git",
        "url": "https://github.com/kukushi/PasteURL.git"
    },
    "bugs": {
        "url": "https://github.com/kukushi/PasteURL/issues"
    },
    "author": "kukushi",
    "license": "MIT",
    "icon": "images/icon.svg",
    "homepage": "https://github.com/kukushi/PasteURL/blob/master/README.md",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pasteURL"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.pasteURL",
                "title": "Paste URL"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+alt+p",
                "command": "extension.pasteURL"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "get-title": "^1.0.1",
        "hyperquest": "^2.1.2",
        "request": "^2.83.0"
    }
}
